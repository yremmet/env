lang(){
    asdf plugin-add $1
    version=$(asdf list-all $1 | grep -v master | grep -v beta | grep -v rc | tail -n 1)
    asdf install $1 $version
    asdf global $1 $version
}


langVersion(){
    asdf plugin-add $1
    version=$(asdf list-all $1 | grep $2 | tail -n 1)
    asdf install $1 $version
    asdf global $1 $version
}